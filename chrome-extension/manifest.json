{
  "manifest_version": 3,
  "name": "ViParser",
  "version": "1.1",
  "description": "A simple product data scraper.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/",
    "*://*/"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}